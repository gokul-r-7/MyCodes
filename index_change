import pandas as pd

# Assuming df is your DataFrame
# First, create a function that moves the row to the desired position
def move_row(df, from_idx, to_idx):
    # Extract the row to be moved
    row = df.iloc[from_idx]
    # Drop the row from the original position
    df = df.drop(from_idx).reset_index(drop=True)
    # Reinsert the row at the new position
    df = pd.concat([df.iloc[:to_idx], row.to_frame().T, df.iloc[to_idx:]]).reset_index(drop=True)
    return df

# Perform the row movements
df = move_row(df, 121, 118)  # Move 122nd row to the 119th position (index 121 to 118)
df = move_row(df, 129, 127)  # Move 130th row to the 128th position (index 129 to 127)
df = move_row(df, 300, 297)  # Move 301st row to the 298th position (index 300 to 297)
df = move_row(df, 308, 305)  # Move 309th row to the 306th position (index 308 to 305)

# df is now updated with rows moved as required
