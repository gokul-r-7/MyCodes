CREATE EXTERNAL TABLE IF NOT EXISTS your_database_name.sample_timekey (
    customer_key DOUBLE,
    account_nbr BIGINT,
    site_id BIGINT,
    res_com_ind STRING,
    customer_status STRING,
    house_type STRING,
    account_guid STRING,
    user_guid_primary STRING,
    employee_flag BIGINT,
    test_account_flag DOUBLE,
    inception_date DATE,
    sale_acquisition_channel DOUBLE,
    registration_date DATE,
    registration_traffic_source_detail STRING,
    registration_traffic_source STRING,
    data_flag BIGINT,
    tv_flag BIGINT,
    phone_flag BIGINT,
    homelife_flag DOUBLE,
    mobile_flag BIGINT,
    pano_flag DOUBLE,
    pano_device DOUBLE,
    easy_pay_flag DOUBLE,
    do_not_call_flag BIGINT,
    do_not_email_flag BIGINT,
    do_not_mail_flag BIGINT,
    do_not_market_flag BIGINT,
    last_contacted_date_ivr_call DATE,
    last_contacted_date_cox_com DATE,
    last_contacted_date_cox_app DATE,
    cox_segment DOUBLE,
    demographic_info1 DOUBLE,
    demographic_info2 DOUBLE,
    time_key DATE
)
ROW FORMAT SERDE 'org.apache.hadoop.hive.serde2.OpenCSVSerde'
WITH SERDEPROPERTIES (
    "separatorChar" = ",",
    "quoteChar" = "\""
)
LOCATION 's3://gokul-test-bucket-07/temporary_files/'
TBLPROPERTIES ('has_encrypted_data'='false');